#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void f(vector<int>arr){
      int mx=INT_MIN,mn=INT_MAX;
      for(int i=0;i<arr.size();i++){
          if(arr[i]>mx)mx=arr[i];
          if(arr[i]<mn)mn=arr[i];
      }
      cout<<mn<<" "<<mx;
  }
};
int main() {
vector<int>arr={10, 20, 15, 2, 23, 90, 67};
solution s;
s.f(arr);



}
