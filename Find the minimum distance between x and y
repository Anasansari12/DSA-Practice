#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
    void f1(vector<int>arr, int x,int y){
        int minimum=INT_MAX;
        for(int i=0;i<arr.size();i++){
            for(int j=i+1;j<arr.size();j++){
                if(x==arr[i] && y==arr[j] || x==arr[j] && y==arr[i]){
                    int diff=abs(j-i);
                    minimum=min(minimum,diff);
                }
            }
        }
        cout<<minimum;
    }
    void f2(vector<int>arr, int x, int y){
        int minimum=INT_MAX,lastindex=-1,lastvalue=0;
        for(int i=0;i<arr.size();i++){
            if(arr[i]==x || arr[i]==y){
                if(lastindex!=-1 && lastvalue!=arr[i]){
                    minimum=min(minimum,i-lastindex);
                }
                lastvalue=arr[i];
                lastindex=i;
            }
        }cout<<minimum;
    }
}; 
int main() {
vector<int>arr={3, 5, 4, 2, 6, 5, 6, 5, 4, 8, 3};
int x=3,y=6;
solution s;
s.f1(arr,x,y);
s.f2(arr,x,y);


}
