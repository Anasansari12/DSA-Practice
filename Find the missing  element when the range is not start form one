#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void f2(vector<int>arr,int &mn, int &mx){
      for(int val:arr){
          mx=max(mx,val);
          mn=min(mn,val);
      }
  }
  void usinghas(vector<int>arr){
      int mx=INT_MIN,mn=INT_MAX;
      f2(arr,mn,mx);
      int size=mx-mn+1;
      vector<int>has(size,0);
      for(int val:arr){
          has[val-mn]=1;
      }
      for(int i=0;i<size;i++){
          if(has[i]==0){
              cout<<mn+i;
          }
      }
  }
  void usingset(vector<int>arr){
      int mx=INT_MIN,mn=INT_MAX;
      f2(arr,mn,mx);
      unordered_set<int>s(arr.begin(),arr.end());
      for(int i=mn;i<=mx;i++){
          if(s.find(i)==s.end()){
              cout<<i;
          }
      }
  }
};
int main() {
vector<int>arr={99,98,97,96,94};
solution s;
s.usinghas(arr);
cout<<endl;
s.usingset(arr);
}
