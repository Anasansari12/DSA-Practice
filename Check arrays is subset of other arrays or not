#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
    bool f1(vector<int>arr1,vector<int>arr2){
        for(int i=0;i<arr2.size();i++){
            bool flag=false;
            for(int j=0;j<arr1.size();j++){
                if(arr2[i]==arr1[j]){
                    flag=true;
                    break;
                }
            }if(flag==false)return false;
        }
        return true;
        }
    bool f2(vector<int>arr1,vector<int>arr2){
         unordered_set<int>s;
         for(int val:arr1)s.insert(val);
         for(int i=0;i<arr2.size();i++){
             if(s.find(arr2[i])==s.end())return false;
         }
         return true;
    }
    bool f3(vector<int>arr1,vector<int>arr2){
        sort(arr1.begin(),arr1.end());
        sort(arr2.begin(),arr2.end());
        int i=0,j=0;
        while(i<arr1.size() && j<arr2.size()){
            if(arr1[i]==arr2[j])j++;
            i++;
            
        }
        return (j==arr2.size());
    }
}; 
int main() {
vector<int>arr1= {1, 2, 3, 4, 5};
vector<int>arr2 = {2, 4, 5};
solution s;
cout<<s.f1(arr1,arr2);
cout<<s.f2(arr1,arr2);
cout<<s.f3(arr1,arr2);

}
