#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
 void f1(vector<int>arr){
     int missing=-1,repreating=-1;
     unordered_map<int,int>fre;
     for(int val:arr)fre[val]++;
     for(int i=1;i<=arr.size();i++){
         if(fre[i]==0)missing=i;
         if(fre[i]==2)repreating=i;
     }
     cout<<missing<<" "<<repreating;
 }
 void f2(vector<int>arr){
     int n=arr.size();
     int s1=n*(n+1)/2;
     int s2=n*(n+1)*(2*n+1)/6;
     int sum1=0,sum2=0;
     for(int i=0;i<arr.size();i++){
         sum1+=arr[i];
         sum2+=arr[i]*arr[i];
     }
     int val1=sum1-s1;
     int val2=sum2-s2;
     val2=(val2/val1);
     int x=(val2+val1)/2;
     int y=x-val1;
     cout<<x<<" "<<y;
 }
      
};
int main() {
vector<int>arr={4, 3, 6, 2, 1, 1};
solution s;
s.f1(arr);
cout<<endl;
s.f2(arr);
}
