#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void f(vector<int>arr, int target){
      int left=0,right=0,sum=0,ans=-1;
      while(right<arr.size()){
          sum+=arr[right];
          while(sum>target){
              sum-=arr[left];
              left++;
          }
          if(sum==target){
              break;
          }
          right++;
      }cout<<left+1<<" "<<right+1;
  }
};
int main() {
vector<int>arr={10,20,30,40,50};
solution s;
s.f(arr,90);
}
