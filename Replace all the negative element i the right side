#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void f(vector<int>&arr){
     int left=0,right=arr.size()-1;
     while(left<right){
         while(arr[right]<0){
             right--;
         }
         if(arr[left]<0){
             swap(arr[left],arr[right]);
             right--;
         }
         left++;
     }
     for(int val:arr)cout<<val;
  }
};
int main() {
vector<int>arr={10,-20,30,-40,50};
solution s;
s.f(arr);
}
