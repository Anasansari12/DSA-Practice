#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void f(vector<int>arr){
      int mx=arr[0],second=INT_MIN;
      for(int i=0;i<arr.size();i++){
          if(arr[i]>mx){
              second=mx;
              mx=arr[i];
          }else if(arr[i]>second && arr[i]!=mx)second=arr[i];
      }
     cout<<second<<" "<<mx;
  }
};
int main() {
vector<int>arr={10, 220, 15, 2, 23, 90, 67};
solution s;
s.f(arr);



}
