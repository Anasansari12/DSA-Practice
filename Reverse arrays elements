#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
    void print(vector<int>arr){
        for(int val:arr)cout<<val<<" ";
    }
    void reverse1(vector<int>&arr){
        int n=arr.size()-1;
        for(int i=0;i<n/2;i++){
            int tem=arr[n-i-1];
            arr[n-i-1]=arr[i];
            arr[i]=tem;
        }
    }
    void reverse2(vector<int>&arr){
        int j=arr.size()-1;
        int i=0;
        while(i<j){
            int tem=arr[i];
            arr[i]=arr[j];
            arr[j]=tem;
            i++,j--;
        }
    }
};
int main() {
vector<int>arr={7,6,5,4,3,2,1};
solution s;
s.reverse1(arr);
cout<<"Methos one"<<endl;
s.print(arr);
s.reverse2(arr);
cout<<"Methos second"<<endl;
s.print(arr);
}
