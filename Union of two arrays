#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
  void f1(vector<int>arr1,vector<int>arr2){
      vector<int>ans;
      for(int i=0;i<arr1.size();i++)ans.push_back(arr1[i]);
      for(int i=0;i<arr2.size();i++){
          bool flag=false;
          for(int j=0;j<arr2.size();j++){
              if(arr1[j]==arr2[i]){
                  flag=true;
              }
          }if(flag==false){
              ans.push_back(arr2[i]);
          }
      }
      for(int val:ans)cout<<val;
  } 
  void f2(vector<int>arr1,vector<int>arr2){
      unordered_set<int>s;
       for(int val:arr1)s.insert(val);
       for(int val:arr2)s.insert(val);
       for(int val:s)cout<<val<<" ";
  }
  void f3(vector<int>arr1,vector<int>arr2){
      sort(arr1.begin(),arr1.end());
      sort(arr2.begin(),arr2.end());
      int n1=arr1.size(),n2=arr2.size();
      int i=0,j=0;
      while(i<n1 && j<n2){
          while(i>0 && arr1[i]==arr1[i-1])i++;
          while(j>0 && arr2[j]==arr2[j-1])j++;
          if(arr1[i]<arr2[j]){
              cout<<arr1[i];
              i++;
          }else if(arr1[i]>arr2[j]){
              cout<<arr2[j];
              j++;
          }else{
              cout<<arr1[i];
              i++,j++;
          }
      }
      while(i<n1){
          cout<<arr1[i];
          i++;
    }
      while(j<n2){
          cout<<arr2[j];
          j++;
      }
  }
};
int main() {
vector<int>arr1={1,2,3,4};
vector<int>arr2={3,4,5,6};
solution s;
s.f1(arr1,arr2);
cout<<endl;
s.f2(arr1,arr2);
cout<<endl;
s.f3(arr1,arr2);
}
