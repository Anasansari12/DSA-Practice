#include <bits/stdc++.h>
using namespace std;
class solution{
   public:
   void f(vector<int>&arr, int target){
       vector<int>ans;
       for(int i=0;i<arr.size();i++){
           for(int j=i+1;j<arr.size();j++){
               if(arr[i]+arr[j]==target){
                   ans.push_back(arr[i]);
                   ans.push_back(arr[j]);
               }
           }
       }
       for(int val:ans)cout<<val<<" ";
   }
};
int main() {
vector<int>arr={1,2,3,4,5,6,7,8};
solution s;
s.f(arr,9);
}
