#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  int pivott(vector<int>&arr, int low, int high){
      int pivot=arr[high];
      int j=low;
      for(int i=low;i<high;i++){
          if(pivot>arr[i]){
              swap(arr[j],arr[i]);
              j++;
          }
      }if(arr[j]>pivot){
          swap(arr[j],pivot);
      }
      return j;
  }
  void quicksort(vector<int>&arr,int low,int high){
      if(low>high)return;
      int pivot=pivott(arr,low,high);
      quicksort(arr,low,pivot-1);
      quicksort(arr,pivot+1,high);
  }
  void print(vector<int>arr){
      for(int val:arr)cout<<val<<" ";
  }
};
int main() {
vector<int>arr={6,5,4,3,2,1};
int n=arr.size();
solution s;
s.quicksort(arr,0,n-1);
s.print(arr);
}
