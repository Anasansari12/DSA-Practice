#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void BUT(vector<int>&arr){
      int i=arr[0];
      int j=0;
      int n=arr.size()-1;
      while(j<=n){
          if(arr[j]!=i){
              cout<<i;
              break;
          }
          i++;j++;
      }
  }
  void better(vector<int>arr){
      int n=arr.size();
      int sumofn=(n*(n+1)/2);
      int sumofelem=0;
      for(int i=0;i<n-1;i++)sumofelem+=arr[i];
      int missing=abs(sumofelem-sumofn);
      cout<<missing;
  }
  void optimize(vector<int>arr){
      int n=arr.size();
      int xoralln=0;
      int xorallelem=0;
      for(int i=1;i<n+1;i++)xoralln^=i;
      for(int i=0;i<n-1;i++)xorallelem^=arr[i];
      int missing=xoralln^xorallelem;
      cout<<missing;
  }
  void f(vector<int>&arr){
      sort(arr.begin(),arr.end());
      BUT(arr);
      cout<<endl;
      better(arr);
      cout<<endl;
      optimize(arr);
  }
};
int main() {
vector<int>arr={1,2,3,5,6};
solution s;
s.f(arr);
}
