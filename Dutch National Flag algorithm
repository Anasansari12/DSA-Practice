#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
 void f(vector<int>&arr){
     int low=0,mid=0,high=arr.size()-1;
     while(mid<=high){
         if(arr[mid]==0){
             swap(arr[mid],arr[low]);
             low++,mid++;
         }else if(arr[mid]==2){
             swap(arr[mid],arr[high]);
             high--;
         }else mid++;
     }
     for(int val:arr)cout<<val;
 }
};
int main() {
vector<int>arr={1, 0, 0, 1, 1, 1, 0, 1,0, 1, 2, 0, 1, 2};
solution s;
s.f(arr);
}
