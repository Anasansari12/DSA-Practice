#include <bits/stdc++.h>
// #include <unordered_map>;
using namespace std;
class solution{
  public:
  void dup(vector<int>arr){
      int ans=-1;
      for(int i=0;i<arr.size();i++){
          for(int j=i+1;j<arr.size();j++){
              if(arr[i]==arr[j] && i!=j){
                  ans=arr[i];
                  break;
              }
          }
      }
      cout<<ans;
  }
  void usinghas(vector<int>arr){
      unordered_map<int,int>m;
      for(int i=0;i<arr.size();i++){
          m[arr[i]]++;
      }
      for(int i=0;i<m.size();i++){
          if(m[arr[i]]==2){
              cout<<arr[i];
              m[arr[i]]=0;
          }
      }
     
  }
  void sorting(vector<int>arr){
      int ans=-1;
      sort(arr.begin(),arr.end());
      for(int i=0;i<arr.size();i++){
          if(i!=0 && arr[i-1]==arr[i]){
              ans=arr[i];
              break;
          }
      }
      cout<<ans;
  }
  void f(vector<int>arr){
      dup(arr);
      usinghas(arr);
      sorting(arr);
  }
};
int main() {
vector<int>arr={1,2,3,4,4,5,6,7};
solution s;
s.f(arr);

}
