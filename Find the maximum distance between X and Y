#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
    void f1(vector<int>arr, int x,int y){
        int maximum=INT_MIN;
        for(int i=0;i<arr.size();i++){
            for(int j=i+1;j<arr.size();j++){
                if(x==arr[i] && y==arr[j] || x==arr[j] && y==arr[i]){
                    int diff=abs(j-i);
                    maximum=max(maximum,j-i);
                }
            }
        }
        cout<<maximum;
    }
    void f2(vector<int>arr, int x, int y){
       int firstx=-1,lastx=-1;
       int firsty=-1,lasty=-1;
       for(int i=0;i<arr.size();i++){
           if(arr[i]==x){
               if(firstx==-1)firstx=i;
               lastx=i;
           }
           if(arr[i]==y){
               if(firsty==-1)firsty=i;
               lasty=i;
           }
       }
       int mx=max(abs(firstx-lasty),abs(firsty-lastx));
       cout<<mx;
       
    }
}; 
int main() {
vector<int>arr={3, 5, 4, 2, 6, 5, 6, 5, 4, 8, 3};
int x=3,y=6;
solution s;
s.f1(arr,x,y);
s.f2(arr,x,y);


}
