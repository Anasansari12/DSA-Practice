#include <bits/stdc++.h>
using namespace std;
class solution{
  public:
  void f(vector<int>arr){
      int mn=INT_MAX,second=arr[0];
      for(int i=0;i<arr.size();i++){
          if(arr[i]<mn){
              second=mn;
              mn=arr[i];
          }else if(arr[i]<second && mn>arr[i] && arr[i]!=mn)second=arr[i];
      }
      cout<<second<<" "<<mn;
  }
};
int main() {
vector<int>arr={10, 220, 15, 2, 23, 90, 67};
solution s;
s.f(arr);



}
