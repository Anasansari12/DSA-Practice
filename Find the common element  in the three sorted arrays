#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
  void f(vector<int>arr1,vector<int>arr2,vector<int>arr3){
      int pre=-1;
      int i=0,j=0,k=0;
      int n1=arr1.size();
      int n2=arr2.size();
      int n3=arr3.size();
      while(i<n1 && j<n2 && k<n3){
          while(i+1<n1 && arr1[i]==arr1[i+1])i++;
          while(j+1<n2 && arr2[j]==arr2[j+1])j++;
          while(k+1<n3 && arr3[k]==arr3[k+1])k++;
          if(arr1[i]==arr2[j] && arr2[j]==arr3[k]){
              if(arr1[i]!=pre){
                  cout<<arr1[i];
                  pre=arr1[i];
                  i++,j++,k++;
              }
          }else if(arr1[i]<arr2[j])i++;
          else if(arr2[j]<arr3[k])j++;
          else k++;
      }
  }
      
};
int main(){
vector<int> arr1 = {1, 5, 10, 20, 40, 80};
vector<int> arr2 = {6, 7, 20, 80, 100};
vector<int> arr3 = {3, 4, 15, 20, 30, 70, 80, 120};
solution s;
s.f(arr1, arr2, arr3);


}
