#include <bits/stdc++.h>
using namespace std;
class soltuion{
  public: 
  void f(vector<int>arr){
      int l=0,r=arr.size()-1;
      while(l<r){
          if(arr[l]==0)l++;
          else if(arr[r]==1)r--;
          else{
              swap(arr[l],arr[r]);
              l++,r--;
          }
      }
      for(int val:arr)cout<<val;
  }
};
int main() {
vector<int> arr = {0, 1, 1, 0, 1, 0};
soltuion s;
s.f(arr);
}
