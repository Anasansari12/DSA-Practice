#include <bits/stdc++.h>
using namespace std;
class solution{
    public:
  void front(vector<int>&arr, int &size){
      for(int i=0;i<size;i++){
          arr[i]=arr[i+1];
      }
      size--;
  }
  void mid(vector<int>&arr,int &size){
      int index=2;
      for(int i=index;i<size;i++){
          arr[i]=arr[i+1];
      }
      size--;
  }
  void end(vector<int>&arr, int &size){
     size--;
  }
  void print(vector<int>&arr,int size){
     for(int i=0;i<=size;i++){
         cout<<arr[i]<<" ";
     }
  }
  void f(vector<int>arr){
      int size=arr.size()-1;
      front(arr,size);
      mid(arr,size);
      end(arr,size);
      print(arr,size);
  } 
};
int main() {
vector<int>arr={1,2,3,4,5};
solution s;
s.f(arr);
}
